{
  "name": "@magnetarjs/plugin-simple-store",
  "version": "0.5.9",
  "sideEffects": false,
  "description": "Magnetar plugin simple store",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "pnpm build --watch",
    "test-and-build": "npm run test && npm run build",
    "test": "ava",
    "test--only": "ava --match='*only:*'"
  },
  "author": "Luca Ban - Mesqueeb",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "TBD",
  "homepage": "https://github.com/cycraft/magnetar#readme",
  "dependencies": {
    "@magnetarjs/core": "workspace:*",
    "@magnetarjs/utils": "workspace:*",
    "copy-anything": "^2.0.3",
    "fast-sort": "^3.1.1",
    "filter-anything": "^2.2.1",
    "is-what": "^3.14.1",
    "merge-anything": "^4.0.2",
    "path-to-prop": "1.0.0",
    "ts-toolbelt": "^9.6.0"
  },
  "devDependencies": {
    "@magnetarjs/test-utils": "^0.1.18",
    "browser-env": "^3.3.0"
  },
  "keywords": [
    "magnetar-plugin",
    "magnetar",
    "magnetarjs",
    "state-management",
    "syncing-solution",
    "db-sync",
    "auto-sync",
    "framework-agnostic",
    "optimistic-ui",
    "vuejs",
    "vuex",
    "vue-sync",
    "vue-state",
    "vue-db",
    "vue-store",
    "firestore",
    "data-store",
    "local-store",
    "remote-store",
    "global-store",
    "database-sync",
    "database-management",
    "database"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycraft/magnetar.git"
  },
  "bugs": {
    "url": "https://github.com/cycraft/magnetar/issues"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "esbuild-register"
    ],
    "timeout": "60s"
  },
  "gitHead": "7358494dc3072b8f1f8bb7ec64ca1ecba610b3e7"
}
