{
  "name": "@magnetarjs/plugin-simple-store",
  "version": "0.0.0",
  "sideEffects": false,
  "description": "A simple local store plugin to use with Vue Sync.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "build": "npm run lint && npm run test && npm run rollup",
    "test": "ava --verbose",
    "test--only": "ava --verbose --match='*only:*'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "rollup": "rollup -c build.js"
  },
  "author": "Luca Ban - Mesqueeb",
  "license": "TBD",
  "homepage": "https://github.com/cycraft/magnetar#readme",
  "peerDependencies": {
    "@magnetarjs/core": "^0.0.0"
  },
  "dependencies": {
    "copy-anything": "^2.0.1",
    "fast-sort": "^2.2.0",
    "filter-anything": "^2.1.2",
    "is-what": "^3.8.0",
    "merge-anything": "^3.0.3",
    "path-to-prop": "0.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@magnetarjs/core": "^0.0.0",
    "@vue/test-utils": "^2.0.0-alpha.6",
    "ava": "^3.8.2",
    "browser-env": "^3.3.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "rollup": "^2.10.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-node": "^8.10.1",
    "ts-toolbelt": "^6.7.7",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.2",
    "vue": "^3.0.0-beta.14"
  },
  "keywords": [
    "vue-sync"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycraft/magnetar.git"
  },
  "bugs": {
    "url": "https://github.com/cycraft/magnetar/issues"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "./test/_setup-browser.js",
      "tsconfig-paths/register",
      "ts-node/register"
    ],
    "timeout": "30s"
  }
}
