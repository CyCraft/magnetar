{
  "name": "@vue-sync/firestore",
  "version": "0.0.0",
  "sideEffects": false,
  "description": "",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "build": "npm run lint && npm run test && npm run rollup",
    "test": "ava --verbose",
    "test--only": "ava --verbose --match='*only:*'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "rollup": "rollup -c build.js"
  },
  "author": "Luca Ban - Mesqueeb",
  "license": "MIT",
  "homepage": "https://github.com/vue-sync/firestore#readme",
  "peerDependencies": {
    "@vue-sync/core": "^0.5.1",
    "firebase": "^7.14.4"
  },
  "dependencies": {
    "flatten-anything": "^2.0.1",
    "ts-toolbelt": "^6.7.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue-sync/core": "^0.5.1",
    "ava": "^3.8.2",
    "copy-anything": "^2.0.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "fast-sort": "^2.2.0",
    "firebase": "^7.14.4",
    "is-what": "^3.8.0",
    "path-to-prop": "0.0.3",
    "rollup": "^2.10.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.2"
  },
  "keywords": [
    "vue-sync"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-sync/firestore.git"
  },
  "bugs": {
    "url": "https://github.com/vue-sync/firestore/issues"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "tsconfig-paths/register",
      "ts-node/register"
    ],
    "timeout": "30s"
  }
}
