{
  "name": "core",
  "version": "0.0.0",
  "description": "Vue-Sync core library.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "build": "npm run lint && npm run test && npm run rollup",
    "test": "ava --verbose",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "rollup": "rollup -c build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-sync/core.git"
  },
  "author": "Luca & Louis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vue-sync/core/issues"
  },
  "homepage": "https://github.com/vue-sync/core#readme",
  "dependencies": {
    "is-what": "^3.6.0",
    "merge-anything": "^3.0.2",
    "ts-toolbelt": "^6.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "ava": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "rollup": "^1.31.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "vue": "^3.0.0-alpha.4",
    "vuex": "^3.1.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
