{
  "name": "@magnetarjs/utils",
  "version": "0.0.1",
  "sideEffects": false,
  "description": "Magnetar utils like a logger for easier development",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "npm run rollup",
    "test": "",
    "rollup": "rimraf dist && rimraf types && rollup -c build.js"
  },
  "author": "Luca Ban - Mesqueeb",
  "license": "TBD",
  "homepage": "https://github.com/cycraft/magnetar#readme",
  "dependencies": {
    "@magnetarjs/core": "^0.0.17",
    "is-what": "^3.14.1"
  },
  "devDependencies": {
    "ava": "^3.15.0"
  },
  "keywords": [
    "magnetar",
    "magnetarjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycraft/magnetar.git"
  },
  "bugs": {
    "url": "https://github.com/cycraft/magnetar/issues"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "timeout": "60s"
  }
