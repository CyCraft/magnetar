{
  "name": "@magnetarjs/plugin-vue3",
  "version": "0.13.2",
  "sideEffects": false,
  "description": "Magnetar plugin vue3",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup src/index.ts --clean --format esm,cjs --dts",
    "dev": "pnpm build --watch",
    "test-and-build": "npm run test && npm run build",
    "test": "ava",
    "test--only": "ava --match='*only:*'"
  },
  "author": "Luca Ban - Mesqueeb",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "TBD",
  "homepage": "https://github.com/cycraft/magnetar#readme",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@magnetarjs/types": "workspace:*",
    "@magnetarjs/utils": "workspace:*",
    "copy-anything": "^3.0.2",
    "filter-anything": "^3.0.3",
    "getorset-anything": "^0.0.3",
    "is-what": "^4.1.7",
    "merge-anything": "^5.1.1",
    "path-to-prop": "2.0.2"
  },
  "devDependencies": {
    "@magnetarjs/core": "workspace:*",
    "@magnetarjs/test-utils": "workspace:*",
    "vue": "^3.2.45"
  },
  "keywords": [
    "reactive-store",
    "vue3",
    "magnetar-plugin",
    "magnetar",
    "magnetarjs",
    "state-management",
    "syncing-solution",
    "db-sync",
    "auto-sync",
    "framework-agnostic",
    "optimistic-ui",
    "vuejs",
    "vuex",
    "vue-sync",
    "vue-state",
    "vue-db",
    "vue-store",
    "firestore",
    "data-store",
    "local-store",
    "remote-store",
    "global-store",
    "database-sync",
    "database-management",
    "database"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycraft/magnetar.git"
  },
  "bugs": {
    "url": "https://github.com/cycraft/magnetar/issues"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "esbuild-register"
    ],
    "timeout": "60s"
  },
  "gitHead": "7358494dc3072b8f1f8bb7ec64ca1ecba610b3e7"
}
